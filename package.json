{
    "author": "RhiTech <tech@rickhanseninstitute.org>",
    "bugs": {
        "url": "https://github.com/rhi-isncsci/ui/issues"
    },
    "description": "Web components package for the ISNCSCI project.",
    "devDependencies": {
        "@webcomponents/webcomponentsjs": "^2.1.3",
        "concurrently": "^4.0.1",
        "fs-extra": "^7.0.0",
        "http-server": "^0.11.1",
        "rimraf": "^2.6.2",
        "rollup": "^0.67.0",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-terser": "^3.0.0",
        "rollup-plugin-typescript2": "^0.17.2",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6"
    },
    "homepage": "https://github.com/rhi-isncsci/ui",
    "keywords": [
        "Rick",
        "Hansen",
        "Institute",
        "custom elements",
        "components",
        "ISNCSCI",
        "algorithm"
    ],
    "license": "Apache-2.0",
    "main": "./umd/index.js",
    "module": "./index.js",
    "name": "rhi-isncsci-ui",
    "private": false,
    "repository": "https://github.com/rhi-isncsci/ui.git",
    "scripts": {
        "prebuild": "rimraf ./demo/scripts",
        "build": "rollup -c",
        "postbuild": "node postbuild/index",
        "prebuild:prod": "rimraf ./dist",
        "build:prod": "rollup -c --environment BUILD:production",
        "postbuild:prod": "node -e \"process.env.BUILD='production'; require('./postbuild/index')\"",
        "preserve": "npm run build",
        "serve": "concurrently \"npm start\" \"rollup -c --watch\" --kill-others-on-fail",
        "start": "http-server ./demo -o -c-1 -p 8081",
        "tslint": "tslint -p ."
    },
    "types": "./index.d.ts",
    "version": "0.0.6"
}
